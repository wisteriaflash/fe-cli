{
  "name": "fe-cli",
  "version": "0.0.1",
  "description": "fe commond tools",
  "main": "lib/index.js",
  "bin": {
    "fe": "lib/index.js"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' --files src/index.ts",
    "prebuild": "cross-env NODE_ENV=prod ts-node config/build.ts",
    "build": "tsc --project tsconfig-build.json",
    "test": "jest --config jestconfig.json",
    "link:pkg": "npm run build && npm link",
    "unlink:pkg": "npm unlink fe-cli"
  },
  "author": "wisteria",
  "license": "ISC",
  "dependencies": {
    "chalk": "^4.0.0",
    "clear": "^0.1.0",
    "commander": "^5.0.0",
    "cross-env": "^7.0.2",
    "figlet": "^1.3.0",
    "fs": "^0.0.1-security",
    "fs-extra": "^9.0.0",
    "inquirer": "^7.1.0",
    "log-symbols": "^3.0.0",
    "minimist": "^1.2.6",
    "mustache": "^4.0.1",
    "ora": "^4.0.3",
    "path": "^0.12.7",
    "shelljs": "^0.8.3"
  },
  "devDependencies": {
    "@types/clear": "^0.1.0",
    "@types/debug": "^4.1.5",
    "@types/figlet": "^1.2.0",
    "@types/fs-extra": "^8.1.0",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^25.2.1",
    "@types/minimist": "^1.2.0",
    "@types/mustache": "^4.0.1",
    "@types/node": "^13.11.0",
    "@types/shelljs": "^0.8.7",
    "cz-conventional-changelog": "3.1.0",
    "debug": "^4.1.1",
    "jest": "^25.2.7",
    "nodemon": "^2.0.2",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
